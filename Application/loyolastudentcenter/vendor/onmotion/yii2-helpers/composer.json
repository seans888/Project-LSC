{
    "name": "onmotion/yii2-helpers",
    "description": "The helpers for Yii2 application",
    "type": "yii2-extension",
    "keywords": ["yii2"," helpers"," translator"," ru2en"],
    "license": "GPL-3.0",
    "homepage": "https://github.com/onmotion/yii2-helpers",
    "authors": [
        {
            "name": "Alexandr Kozhevnikov",
            "email": "onmotion1@gmail.com",
            "homepage": "http://kozhevnikov.me",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "onmotion\\helpers\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
